{% extends "base.html.twig" %}
{% block title %} Equipe | Kina {% endblock %}
{% block page_title_title %}L'équipe{% endblock %}
{% block page_title_second_title_full %}{% endblock %}
{% block page_title_content %}{% endblock %}

{% block content %}
<!-- teachers -->
<section class="section">
  <div data-ref="mixitup-target" class="container">
    <div class="row">
      <div class="col-12">
        <!-- teacher category list -->
        <ul class="list-inline text-center filter-controls mb-5">
          <li class="list-inline-item m-3 text-uppercase" data-filter=".ROLE_USER_ANIMATOR">Animation</li>
          <li class="list-inline-item m-3 text-uppercase" data-filter=".ROLE_USER_TRADUCTOR">Traduction</li>          
        </ul>
      </div>
    </div>
    <!-- teacher list -->
    <div class="row" data-ref="mixitup-container">
      <!-- teacher -->
      {% for teammate in team %}
        
      <div data-ref="mixitup-target" class="col-lg-4 col-sm-6 mb-5 {{(teammate.roles ? is empty) ? teammate.roles|join(' ') : '' }}">
        <div class="card border-0 rounded-0 hover-shadow">
          <img class="card-img-top rounded-0" src="{{ asset ('assets/images/teachers/teacher-1.jpg') }}" alt="teacher">
          <div class="card-body">
            <h4 class="card-title">{{teammate.firstName}} {{teammate.lastName}}</h4>           
           
            {% if 'ROLE_USER_ANIMATOR' in teammate.roles and 'ROLE_USER_ANIMATOR' in teammate.roles %}
            <p>Animateur | Traducteur</p>                      
            {% elseif 'ROLE_USER_ANIMATOR' in teammate.roles %}
            <p>Animateur </p>  
            <p>Champ d'activité: {{teammate.activityExecutions|join(', ', ' et ') ?? ' - '}}</p>  
            {% elseif 'ROLE_USER_TRADUCTOR' in teammate.roles %}
            <p>Traducteur</p> 
            {% else %}
            <p> - </p>    
            {% endif %} 

            <ul class="list-inline">
              <li class="list-inline-item"><a class="text-color" href="#"><i class="ti-facebook"></i></a></li>
              <li class="list-inline-item"><a class="text-color" href="#"><i class="ti-linkedin"></i></a></li>
            </ul>
          </div>
        </div>
      </div>
      {% endfor %}
